	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">GHRSST+Level+3C+Indian-Ocean+(IO)+sub-skin+Sea+Surface+Temperature+from+the+Spinning+Enhanced+Visible+and+Infrared+Imager+(SEVIRI)+on+MSG1+in+GDS2+format+produced+by+OSISAF</SimpleData>
		<SimpleData name="cmr_concept_id">PODAAC</SimpleData>
