	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">GHRSST+Level+2P+Western+Atlantic+Regional+Skin+Sea+Surface+Temperature+from+the+Geostationary+Operational+Environmental+Satellites+(GOES)+Imager+on+the+GOES-13+satellite+(GDS+version+2)</SimpleData>
		<SimpleData name="cmr_concept_id">PODAAC</SimpleData>
