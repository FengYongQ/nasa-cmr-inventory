	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">GHRSST+Level+4+MUR+0.25deg+Global+Foundation+Sea+Surface+Temperature+Analysis+(v4.2)</SimpleData>
