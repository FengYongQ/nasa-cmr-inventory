  <Placemark>
	<name>collection</name>
	<description>C1664148256-PODAAC</description>
	<Style><LineStyle><color>ff0000ff</color></LineStyle><PolyStyle><fill>0</fill></PolyStyle></Style>
	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">JPL+GRACE+and+GRACE-FO+Mascon+Ocean,+Ice,+and+Hydrology+Equivalent+Water+Height+JPL+Release+06+Version+02</SimpleData>
