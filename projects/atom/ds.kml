	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">ATom: L2 Photolysis Frequencies from NCAR CCD Actinic Flux Spectroradiometers (CAFS)</SimpleData>
		<SimpleData name="cmr_concept_id">ORNL_DAAC</SimpleData>
		<SimpleData name="cmr_native_id">1</SimpleData>
		<SimpleData name="cmr_provider_id">2019-10-02T12:30:50Z</SimpleData>
