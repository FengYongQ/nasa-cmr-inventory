	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">Snow Depth, Stratigraphy, and Temperature in Wrangell St Elias NP, Alaska, 2016-2018</SimpleData>
		<SimpleData name="cmr_concept_id">ORNL_DAAC</SimpleData>
