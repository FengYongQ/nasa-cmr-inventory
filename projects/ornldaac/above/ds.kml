	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">ABoVE: Wildfire Carbon Emissions and Burned Plot Characteristics, NWT, CA, 2014-2016</SimpleData>
		<SimpleData name="cmr_concept_id">ORNL_DAAC</SimpleData>
