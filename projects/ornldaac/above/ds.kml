		<SimpleData name="cmr_concept_id">ORNL_DAAC</SimpleData>
		<SimpleData name="cmr_native_id">3</SimpleData>
		<SimpleData name="cmr_provider_id">2019-04-26T20:14:14Z</SimpleData>
